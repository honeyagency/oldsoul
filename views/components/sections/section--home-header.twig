<section class="section--home-header dsk--flex flex--wrap justify--between full-height hidden--mob">
	<div class="grid--one grid--sm-half order--2 nog relative flex flex--column flex--wrap justify--end overflow-no">
		<img class="inline-background lazy" src="{{home.header.rightimage|resize(363,330)}}" data-src="{{home.header.rightimage|resize(1430,1300)}}" alt="">
		<div class="bg--brown-dark text--center spacing--vert-2 spacing--horiz-1 relative z-99">
			<h2 class="color--white transform--no margin--top-1 margin--bot-2">{{home.header.title}}</h2>
			{% for link in home.header.links %}
				{% if link.url %}<a href="{{link.url}}" target="{{link.target}}" class="button font--smaller center {% if loop.last %}margin--bot-3{% else %}margin--bot-half{% endif %}">{{link.title}}</a>{% endif %}
			{% endfor %}	
			<i class="icon-logo-monogram hidden--mob" data-grunticon-embed></i>
		</div>
	</div>
	<div class="grid--one grid--sm-half order--1 nog relative overflow-no hidden--mob">
		<img class="inline-background lazy" src="{{home.header.leftimage|resize(360,377)}}" data-src="{{home.header.leftimage|resize(1440,1540)}}" alt="">
	</div>
</section>