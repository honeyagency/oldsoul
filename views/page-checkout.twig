{% extends "base.twig" %}
{% block content %}
	<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
		<section class="article-content max-wrap margin--center">
			<h1 class="article-h1">{{post.title}}</h1>
			{{ function('do_shortcode', '[woocommerce_checkout]') }}
		</section>
	</article>
{% endblock %}